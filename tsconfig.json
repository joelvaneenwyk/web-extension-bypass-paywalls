{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Modules */
    "module": "commonjs",
    "moduleDetection": "force",
    "noEmitOnError": true,
    "outDir": "src/js",
    "sourceMap": true,
    "strict": false,
    /* Visit https://aka.ms/tsconfig to read more about this file */
    /* Language and Environment */
    "target": "es2016"
  },
  "include": [
    "./src/ts/*.ts"
  ]
}
