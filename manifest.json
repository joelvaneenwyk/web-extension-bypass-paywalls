{
  "background": {
    "scripts": [
      "src/js/common.js",
      "src/js/sites.js",
      "src/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "src/icons/bypass.png"
    },
    "default_popup": "src/html/popup.html"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "Bypass News Sites' Paywalls",
  "homepage_url": "https://github.com/joelvaneenwyk/bypass-paywalls-extension/blob/main/README.md",
  "icons": {
    "128": "src/icons/bypass.png"
  },
  "key": "",
  "manifest_version": 3,
  "name": "Bypass Paywalls",
  "options_ui": {
    "chrome_style": false,
    "page": "src/html/options.html"
  },
  "permissions": [
    "cookies",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "Bypass Paywall",
  "update_url": "https://raw.githubusercontent.com/joelvaneenwyk/bypass-paywalls-extension/main/src/updates/updates.xml",
  "version": "1.8.1"
}
